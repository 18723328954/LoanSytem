<mapper namespace="com.dafuweng.dao.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dafuweng.entity.Log">
        <result column="user_id" property="userId" />
        <result column="username" property="username" />
